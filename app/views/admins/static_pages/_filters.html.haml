.panel
  = form_tag admins_static_pages_path, :method=> "get", :remote => true , :id => "static_page_search" do
    = text_field_tag :name,nil,:class => "form-control filter_txt_field",placeholder: 'Name'
    = text_field_tag :label,nil,:class => "form-control filter_txt_field",placeholder: 'Label'
    = text_field_tag :page_route,nil,:class => "form-control filter_txt_field",placeholder: 'Page Route'
    = button_tag(type: "submit", class: "btn btn-info filter_btn") do
      Filter
      %i.glyphicon.glyphicon-search   
    = link_to "Clear Filter", request.path, class:"btn btn-info filter_btn"